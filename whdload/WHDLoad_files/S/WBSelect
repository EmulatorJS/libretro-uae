IF NOT "$WBDIR" EQ ""
   C:RequestFile $WBDIR/#?.info >ENV:WBINFO
ELSE
   C:RequestFile #?.info >ENV:WBINFO
ENDIF
IF NOT "$WBINFO" EQ "" >NIL:
   C:List $WBINFO QUICK NOHEAD LFORMAT %E TO ENV:WBEXT
   IF "$WBEXT" EQ "info"
      C:List $WBINFO QUICK NOHEAD FILES LFORMAT %F%M TO ENV:WBRUN
      C:List "$WBRUN#?" QUICK NOHEAD DIRS LFORMAT %F%M TO ENV:WBDIR
      IF NOT "$WBDIR" EQ ""
         C:Execute S:WBSelect
      ELSE
         C:WBRun "$WBRUN" SYNC >NIL:
      ENDIF
   ELSE
      C:List $WBINFO QUICK NOHEAD LFORMAT %F%N TO ENV:WBRUN
      $WBRUN >NIL:
   ENDIF
   UNSETENV WBEXT
ENDIF
UNSETENV WBINFO
